        "@mui/material": "^6.4.1",
        "@types/smoothie": "^1.25.29",
        "axios": "^1.8.4",
        "baffle": "^0.3.6",
        "cross-env": "^7.0.3",
        "d3": "^7.9.0",
        "gsap": "^3.12.7",
        "motion": "^12.23.12",
        "papaparse": "^5.5.3",
        "plotly.js": "^2.25.2",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-plotly.js": "^2.6.0",
