{
    "name": "jerryxf",
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "cross-env NODE_ENV=development vite --host",
        "prebuild": "npm install && npm run lint",
        "build": "tsc -b && vite build",
        "lint": "eslint . --report-unused-disable-directives --max-warnings 10",
        "lint:fix": "eslint . --report-unused-disable-directives --fix",
        "type-check": "tsc --noEmit",
        "preview": "vite preview",
        "start": "cross-env NODE_ENV=production serve -s dist -l 3000",
        "test": "echo \"Error: no test specified\" && exit 1",
        "analyze": "npm run build && npx vite-bundle-analyzer"
    },
